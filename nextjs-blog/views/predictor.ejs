 <div className={styles.container}>
      <Head>
        <title>Predictor</title>
        <meta name="description" content="Generated by create next app" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link rel="icon" href="./OP.jpg" />
      </Head>

      <header>
      <img class="logo" src='./OP_logo_3.jpg'></img>
        <nav> 
            <ul class="nav_links">
                <li><a href='/'>Home</a></li>
                <li><a href='/predictor'>Predictor</a></li>
            </ul>   
        </nav>
        <p class="profile">Name</p>
      </header>

      <main className={styles.main}>
            <section>
                <div class="survey_container">
                    <div id="title">
                        <h2>Predicting obesity levels based on eating habits and physical condition</h2>
                    </div>
                    <form id="survey-form">
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>What is your gender?</label>
                            </div>
                            <div class="input-tab">
                                <div class="radio_choice">
                                    <input type="radio" name="suggest1"></input>
                                    <label>Female</label>
                                </div>
                                <div class="radio_choice">
                                    <input type="radio" name="suggest1"></input>
                                    <label>Male</label>
                                </div>  
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>What is your age?</label>
                            </div>
                            <div class="input-tab">
                                <input type="number" name="Age" placeholder='Enter your age'></input>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>What is your height?</label>
                            </div>
                            <div class="input-tab">
                                <input type="number" step="any" name="Height" placeholder='Enter your height in meters'></input>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>What is your weight?</label>
                            </div>
                            <div class="input-tab">
                                <input type="number" step="any" name="Weight" placeholder='Enter your weight in kilograms'></input>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Has a family member suffered or suffers from overweight?</label>
                            </div>
                            <div class="input-tab">
                                <div class="radio_choice">
                                    <input type="radio" name="suggest2"></input>
                                    <label>Yes</label>
                                </div>
                                <div class="radio_choice">
                                    <input type="radio" name="suggest2"></input>
                                    <label>No</label>
                                </div>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Do you eat high caloric food frequently?</label>
                            </div>
                            <div class="input-tab">
                                <div class="radio_choice">
                                    <input type="radio" name="suggest3"></input>
                                    <label>Yes</label>
                                </div>
                                <div class="radio_choice">
                                    <input type="radio" name="suggest3"></input>
                                    <label>No</label>
                                </div>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Do you usually eat vegetables in your meals?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>Never</option>
                                    <option>Sometimes</option>
                                    <option>Always</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>How many main meals do you have daily?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>Between 1 and 2</option>
                                    <option>Three</option>
                                    <option>More than three</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Do you eat any food between meals?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>No</option>
                                    <option>Sometimes</option>
                                    <option>Frequently</option>
                                    <option>Always</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Do you smoke?</label>
                            </div>
                            <div class="input-tab">
                                <div class="radio_choice">
                                    <input type="radio" name="suggest4"></input>
                                    <label>Yes</label>
                                </div>
                                <div class="radio_choice">
                                    <input type="radio" name="suggest4"></input>
                                    <label>No</label>
                                </div>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>How much water do you drink daily?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>Less than a liter</option>
                                    <option>Between 1 and 2L</option>
                                    <option>More than 2L</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Do you monitor the calories you eat daily?</label>
                            </div>
                            <div class="input-tab">
                                <div class="radio_choice">
                                    <input type="radio" name="suggest5"></input>
                                    <label>Yes</label>
                                </div>
                                <div class="radio_choice">
                                    <input type="radio" name="suggest5"></input>
                                    <label>No</label>
                                </div>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>How often do you have physical activity?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>I do not have</option>
                                    <option>1 or 2 days</option>
                                    <option>2 or 4 days</option>
                                    <option>4 or 5 days</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>How much time do you use technological devices such as cell phone, videogames, television, computer and others?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>0-2 hours</option>
                                    <option>3-5 hours</option>
                                    <option>more than 5 hours</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>How often do you drink alcohol?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>I do not drink</option>
                                    <option>Sometimes</option>
                                    <option>Frequently</option>
                                    <option>Always</option>
                                </select>
                            </div>
                        </div>
                        <div class="row-tab">
                            <div class="label-tab">
                                <label>Which transportation do you usually use?</label>
                            </div>
                            <div class="select-tab">
                                <select id="select-box">
                                    <option>Select an option</option>
                                    <option>Automobile</option>
                                    <option>Motorbike</option>
                                    <option>Bike</option>
                                    <option>Public transportation</option>
                                    <option>Walking</option>
                                </select>
                            </div>
                        </div>
                        <div class="predict_btn">
                            <button type="button">Predict</button>
                        </div>
                    </form>
                </div>
            </section>
            
            
 
      </main>

    </div>