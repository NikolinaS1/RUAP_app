        <section>
            <div class="survey_container">
                <div id="title">
                    <h2 class="text-center mb-4 mt-4 text-secondary">Predicting obesity levels based on eating habits and physical condition</h2>
                </div>
                    <div class="container h-100">
      <div class="row h-100 justify-content-center align-items-center">
                <form class="survey-form col-md-4" method="POST">
                    <div class="row-tab">
                        <div class="label-tab">
                            <label>What is your gender?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2 ms-4">
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="Female" name="gender"></input>
                                <label for="gender">Female</label>
                            </div>
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="Male" name="gender"></input>
                                <label for="gender">Male</label>
                            </div>  
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>What is your age?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2">
                            <input class="text form-control", type="number" name="Age" placeholder='Enter your age' min="0", max="120"></input>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>What is your height?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2">
                            <input class="text form-control", type="number" step="any" name="Height" placeholder='Enter your height in meters' min="0", max="2.5"></input>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>What is your weight?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2">
                            <input class="text form-control", type="number" step="any" name="Weight" placeholder='Enter your weight in kilograms', min="0", max="400"></input>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Has a family member suffered or suffers from overweight?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2 ms-4">
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="yes" name="family_history_with_overweight"></input>
                                <label for = "family_history_with_overweight">yes</label>
                            </div>
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="no" name="family_history_with_overweight"></input>
                                <label for = "family_history_with_overweight">no</label>
                            </div>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Do you eat high caloric food frequently?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2 ms-4">
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="yes" name="FAVC"></input>
                                <label for="FAVC">yes</label>
                            </div>
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="no" name="FAVC"></input>
                                <label for="FAVC">no</label>
                            </div>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Do you usually eat vegetables in your meals?</label>
                        </div>
                        <div class="select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="FCVC">
                                <option disabled selected hidden>Select an option</option>
                                <option value="1">Never</option>
                                <option value="2">Sometimes</option>
                                <option value="3">Always</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>How many main meals do you have daily?</label>
                        </div>
                        <div class="select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="NCP">
                                <option disabled selected hidden>Select an option</option>
                                <option value="1">Between 1 and 2</option>
                                <option value="3">Three</option>
                                <option value="4">More than three</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Do you eat any food between meals?</label>
                        </div>
                        <div class="select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="CAEC">
                                <option disabled selected hidden>Select an option</option>
                                <option>no</option>
                                <option>Sometimes</option>
                                <option>Frequently</option>
                                <option>Always</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Do you smoke?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2 ms-4">
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="yes" name="SMOKE"></input>
                                <label for="SMOKE">yes</label>
                            </div>
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="no" name="SMOKE"></input>
                                <label for="SMOKE">no</label>
                            </div>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>How much water do you drink daily?</label>
                        </div>
                        <div class="select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="CH2O">
                                <option disabled selected hidden>Select an option</option>
                                <option value="1">Less than a liter</option>
                                <option value="2">Between 1 and 2L</option>
                                <option value="3">More than 2L</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Do you monitor the calories you eat daily?</label>
                        </div>
                        <div class="input-tab mt-2 mb-2 ms-4">
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="yes" name="SCC"></input>
                                <label for="SCC">yes</label>
                            </div>
                            <div class="radio_choice form-check">
                                <input class="form-check-input" type="radio" value="no" name="SCC"></input>
                                <label for="SCC">no</label>
                            </div>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>How often do you have physical activity?</label>
                        </div>
                        <div class="select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="FAF">
                                <option disabled selected hidden>Select an option</option>
                                <option value="0">I do not have</option>
                                <option value="1">1 or 2 days</option>
                                <option value="2">2 or 4 days</option>
                                <option value="3">4 or 5 days</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>How much time do you use technological devices such as cell phone, videogames, television, computer and others?</label>
                        </div>
                        <div class="select-tab select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="TUE">
                                <option disabled selected hidden>Select an option</option>
                                <option value="0">0-2 hours</option>
                                <option value="1">3-5 hours</option>
                                <option value="2">more than 5 hours</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>How often do you drink alcohol?</label>
                        </div>
                        <div class="select-tab select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="CALC">
                                <option disabled selected hidden>Select an option</option>
                                <option value="no">I do not drink</option>
                                <option>Sometimes</option>
                                <option>Frequently</option>
                                <option>Always</option>
                            </select>
                        </div>
                    </div>
                    <div class="row-tab mt-2 mb-2">
                        <div class="label-tab">
                            <label>Which transportation do you usually use?</label>
                        </div>
                        <div class="select-tab select-tab form-group mt-2 mb-2">
                            <select id="select-box" class="form-control" name="MTRANS">
                                <option disabled selected hidden>Select an option</option>
                                <option>Automobile</option>
                                <option>Motorbike</option>
                                <option>Bike</option>
                                <option value="Public_Transportation">Public transportation</option>
                                <option>Walking</option>
                            </select>
                        </div>
                    </div>
                    <div class="mt-3 mb-3">
                        <div class="d-flex align-items-center justify-content-center">
                            <button class="btn btn-primary" type="submit">Predict</button>
                        </div>
                    </div>
                </form>
                </div>
                </div>
            </div>
        </section>
    <!-- </main> -->
<!--</div>-->

